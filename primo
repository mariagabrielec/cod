primo = int(input('Digite um número:')) #pede o número para o usuário
contador = 0 #variavel para armazenar a quantidade de números que
                #o número passado pelo usuário é divisivel
for i in range(1,primo+1): #numero de 1 até o passado pelo usuário para divisao
    if primo % i == 0: #se o resto da divisão for igual a 0 significa que é divisivel
        contador += 1 #então acrescenta na soma de qntd de números divisiveis
if contador == 2: #se a qntd de numeros divisiveis for igual a 2
    print('É primo!')
else:
    print('Não é primo!')
